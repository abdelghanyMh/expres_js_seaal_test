{
  "name": "express-pug-tailwind-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run dockup && npm run build:css",
    "start": "NODE_ENV=production npm run build:css && node ./bin/www",
    "build:css": "postcss src/css/app.css -o assets/styles.css",
    "lint": "eslint .",
    "dockup": "docker compose up -d",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:init": "npm run db:create && npm run db:migrate &&  npm run db:seed",
    "listen": "nodemon ./bin/www"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "debug": "^4.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "node-cron": "^3.0.3",
    "pug": "^3.0.2",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "cssnano": "^5.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.0",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^2.1.2"
  }
}
